{"ast":null,"code":"import \"core-js/modules/es.regexp.flags.js\";\nexport default {\n  name: \"countriesDetail\",\n  props: [\"common\"],\n\n  async mounted() {\n    fetch(`https://restcountries.com/v3.1/name/${this.common}`).then(res => res.json()).then(response => {\n      response.forEach(con => {\n        this.name = con.name.common;\n        this.population = con.population;\n        this.region = con.region;\n        this.subregion = con.subregion;\n        this.capital = con.capital[0];\n        this.borders = con.borders;\n        this.border = Object.keys(con.borders).forEach(border => {\n          return border;\n        });\n        this.svg = con.flags.svg;\n      });\n      this.country = response;\n      console.log(this.country);\n    }).catch(error => {\n      console.log(\"Looks like there was a problem: \\n\", error);\n    });\n  },\n\n  // created() {\n  //   this.currentPage = this.$route.params.common;\n  // },\n  data() {\n    return {\n      name: null,\n      population: null,\n      region: null,\n      subregion: null,\n      capital: null,\n      borders: null,\n      border: null,\n      svg: null,\n      currentPage: null\n    };\n  }\n\n};","map":{"version":3,"mappings":";AA4CA,eAAe;EACbA,IAAI,EAAE,iBADO;EAEbC,KAAK,EAAE,CAAC,QAAD,CAFM;;EAIb,MAAMC,OAAN,GAAgB;IACdC,KAAK,CAAE,uCAAsC,KAAKC,MAAO,EAApD,CAAL,CACGC,IADH,CACSC,GAAD,IAASA,GAAG,CAACC,IAAJ,EADjB,EAEGF,IAFH,CAESG,QAAD,IAAc;MAClBA,QAAQ,CAACC,OAAT,CAAkBC,GAAD,IAAS;QACxB,KAAKV,IAAL,GAAYU,GAAG,CAACV,IAAJ,CAASI,MAArB;QACA,KAAKO,UAAL,GAAkBD,GAAG,CAACC,UAAtB;QACA,KAAKC,MAAL,GAAcF,GAAG,CAACE,MAAlB;QACA,KAAKC,SAAL,GAAiBH,GAAG,CAACG,SAArB;QACA,KAAKC,OAAL,GAAeJ,GAAG,CAACI,OAAJ,CAAY,CAAZ,CAAf;QACA,KAAKC,OAAL,GAAeL,GAAG,CAACK,OAAnB;QACA,KAAKC,MAAL,GAAcC,MAAM,CAACC,IAAP,CAAYR,GAAG,CAACK,OAAhB,EAAyBN,OAAzB,CAAkCO,MAAD,IAAY;UACzD,OAAOA,MAAP;QACD,CAFa,CAAd;QAGA,KAAKG,GAAL,GAAWT,GAAG,CAACU,KAAJ,CAAUD,GAArB;MACD,CAXD;MAYA,KAAKE,OAAL,GAAeb,QAAf;MACAc,OAAO,CAACC,GAAR,CAAY,KAAKF,OAAjB;IACD,CAjBH,EAkBGG,KAlBH,CAkBUC,KAAD,IAAW;MAChBH,OAAO,CAACC,GAAR,CAAY,oCAAZ,EAAkDE,KAAlD;IACD,CApBH;EAqBD,CA1BY;;EA4Bb;EACA;EACA;EAEAC,IAAI,GAAG;IACL,OAAO;MACL1B,IAAI,EAAE,IADD;MAELW,UAAU,EAAE,IAFP;MAGLC,MAAM,EAAE,IAHH;MAILC,SAAS,EAAE,IAJN;MAKLC,OAAO,EAAE,IALJ;MAMLC,OAAO,EAAE,IANJ;MAOLC,MAAM,EAAE,IAPH;MAQLG,GAAG,EAAE,IARA;MASLQ,WAAW,EAAE;IATR,CAAP;EAWD;;AA5CY,CAAf","names":["name","props","mounted","fetch","common","then","res","json","response","forEach","con","population","region","subregion","capital","borders","border","Object","keys","svg","flags","country","console","log","catch","error","data","currentPage"],"sourceRoot":"","sources":["D:\\Challenges\\REST Countries API\\reast-countries-api\\src\\views\\countriesDetail.vue"],"sourcesContent":["\r\n<template>\r\n  <div class=\"detailOfCountries\" :value=\"value\">\r\n    <br />\r\n    <br />\r\n    <div class=\"back\">\r\n      <router-link to=\"/\">\r\n        <ion-icon name=\"arrow-back-sharp\"></ion-icon>\r\n        <p>Back</p>\r\n      </router-link>\r\n    </div>\r\n    <div class=\"detas\">\r\n      <div class=\"imgDeta\" @click=\"goet\">\r\n        <img :src=\"svg\" alt=\"\" />\r\n      </div>\r\n      <div class=\"infDeta\">\r\n        <div class=\"labRL\">\r\n          <div class=\"leftInf\">\r\n            <span>{{ name }}</span>\r\n            <p>Native name: dfdsfff</p>\r\n            <p>population: {{ population }}</p>\r\n            <p>Region: {{ region }}</p>\r\n            <p>Sub region: {{ subregion }}</p>\r\n            <p>Capital: {{ capital }}</p>\r\n          </div>\r\n          <div class=\"rightInf\">\r\n            <p>Top Level Domain: cd</p>\r\n            <p>Currencies: dffff</p>\r\n            <p>Languages: carld, dfh, kfsfdf</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"bordsInf\">\r\n          <p>border Countries:</p>\r\n          <a v-for=\"border in borders\" :key=\"border\" to=\"/\">{{ border }}</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n\r\n<script>\r\nexport default {\r\n  name: \"countriesDetail\",\r\n  props: [\"common\"],\r\n\r\n  async mounted() {\r\n    fetch(`https://restcountries.com/v3.1/name/${this.common}`)\r\n      .then((res) => res.json())\r\n      .then((response) => {\r\n        response.forEach((con) => {\r\n          this.name = con.name.common;\r\n          this.population = con.population;\r\n          this.region = con.region;\r\n          this.subregion = con.subregion;\r\n          this.capital = con.capital[0];\r\n          this.borders = con.borders;\r\n          this.border = Object.keys(con.borders).forEach((border) => {\r\n            return border;\r\n          });\r\n          this.svg = con.flags.svg;\r\n        });\r\n        this.country = response;\r\n        console.log(this.country);\r\n      })\r\n      .catch((error) => {\r\n        console.log(\"Looks like there was a problem: \\n\", error);\r\n      });\r\n  },\r\n\r\n  // created() {\r\n  //   this.currentPage = this.$route.params.common;\r\n  // },\r\n\r\n  data() {\r\n    return {\r\n      name: null,\r\n      population: null,\r\n      region: null,\r\n      subregion: null,\r\n      capital: null,\r\n      borders: null,\r\n      border: null,\r\n      svg: null,\r\n      currentPage: null,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n\r\n\r\n\r\n<style lang=\"scss\">\r\n.detailOfCountries {\r\n  width: 100%;\r\n  height: 100vh;\r\n}\r\n\r\n.back {\r\n  display: block;\r\n  padding: 10px;\r\n  background-color: var(--elements);\r\n  font-weight: 600;\r\n  box-shadow: 0 0 5px black;\r\n  max-width: 126px;\r\n  justify-content: space-between;\r\n  border-radius: 7px;\r\n  margin: 0 0 15px 125px;\r\n  a {\r\n    text-decoration: none;\r\n    color: var(--Blue-text);\r\n    display: flex;\r\n    gap: 10px;\r\n  }\r\n}\r\n\r\n.detas {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-evenly;\r\n  padding: 40px 60px;\r\n  .imgDeta {\r\n    width: 19vw;\r\n    img {\r\n      width: 100%;\r\n      height: 100%;\r\n      border-radius: 7px;\r\n    }\r\n  }\r\n  .infDeta {\r\n    display: flex;\r\n    padding: 15px;\r\n    justify-content: space-around;\r\n    flex-direction: column;\r\n    width: 500px;\r\n    .labRL {\r\n      display: flex;\r\n      flex-direction: row;\r\n      justify-content: space-between;\r\n      .leftInf {\r\n        color: var(--Blue-text);\r\n        display: flex;\r\n        flex-direction: column;\r\n        gap: 10px;\r\n        font-size: 0.8rem;\r\n        span {\r\n          font-weight: 800;\r\n          font-size: 1.5rem;\r\n          margin: 10px;\r\n        }\r\n      }\r\n      .rightInf {\r\n        color: var(--Blue-text);\r\n        font-size: 0.8rem;\r\n        display: flex;\r\n        flex-direction: column;\r\n        gap: 10px;\r\n      }\r\n    }\r\n    .bordsInf {\r\n      display: flex;\r\n      flex-direction: row;\r\n      align-items: center;\r\n      color: var(--Blue-text);\r\n      gap: 7px;\r\n      flex-wrap: wrap;\r\n      a {\r\n        margin: 0 10px;\r\n        padding: 9px;\r\n        color: var(--Blue-text);\r\n        box-shadow: 0 0 5px black;\r\n        background-color: var(--elements);\r\n        border-radius: 4px;\r\n        text-decoration: none;\r\n        font-size: 0.8rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}