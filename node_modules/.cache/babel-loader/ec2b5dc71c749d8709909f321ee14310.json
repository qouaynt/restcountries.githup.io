{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"HomeView\",\n\n  async mounted() {\n    this.pending = true;\n    axios.get(\"https://restcountries.com/v3.1/all\").then(response => {\n      this.countries = response.data;\n      this.countries.forEach(con => {\n        this.capital = con.capital[0];\n        console.log(this.capital);\n      });\n    }).catch(error => this.error = error).finally(() => {\n      this.pending = false;\n    });\n  },\n\n  computed: {\n    filterAllCountries: function () {\n      return this.countries.filter(country => {\n        if (this.region === \"\" || this.region === \"All Regions\") {\n          return this.countries.length ? Object.keys(this.countries[0]).some(key => (\"\" + country[key]).toLowerCase().includes(this.search)) : true;\n        } else if (this.search !== \"\") {\n          return this.countries.length ? Object.keys(this.countries[0]).some(key => (\"\" + country[key]).toLowerCase().includes(this.search)) : true;\n        } else {\n          return country.region.match(this.region);\n        }\n      });\n    }\n  },\n  methods: {\n    handleFilterClick() {\n      setTimeout(() => {\n        this.showFilter = !this.showFilter;\n        this.showAllRegion = true;\n      });\n    },\n\n    formatNumbers(value) {\n      return value.toLocaleString();\n    }\n\n  },\n\n  data() {\n    return {\n      countries: [],\n      search: \"\",\n      ulFilter: false,\n      showFilter: false,\n      showAllRegion: false,\n      region: \"\",\n      pending: false,\n      error: null,\n      capital: null\n    };\n  }\n\n};","map":{"version":3,"mappings":";AAmIA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAGb,MAAMC,OAAN,GAAgB;IACd,KAAKC,OAAL,GAAe,IAAf;IACAH,KAAI,CACDI,GADH,CACO,oCADP,EAEGC,IAFH,CAESC,QAAD,IAAc;MAClB,KAAKC,SAAL,GAAiBD,QAAQ,CAACE,IAA1B;MACA,KAAKD,SAAL,CAAeE,OAAf,CAAwBC,GAAD,IAAS;QAC9B,KAAKC,OAAL,GAAeD,GAAG,CAACC,OAAJ,CAAY,CAAZ,CAAf;QACAC,OAAO,CAACC,GAAR,CAAY,KAAKF,OAAjB;MACD,CAHD;IAID,CARH,EASGG,KATH,CASUC,KAAD,IAAY,KAAKA,KAAL,GAAaA,KATlC,EAUGC,OAVH,CAUW,MAAM;MACb,KAAKb,OAAL,GAAe,KAAf;IACD,CAZH;EAaD,CAlBY;;EAoBbc,QAAQ,EAAE;IACRC,kBAAkB,EAAE,YAAY;MAC9B,OAAO,KAAKX,SAAL,CAAeY,MAAf,CAAuBC,OAAD,IAAa;QACxC,IAAI,KAAKC,MAAL,KAAgB,EAAhB,IAAsB,KAAKA,MAAL,KAAgB,aAA1C,EAAyD;UACvD,OAAO,KAAKd,SAAL,CAAee,MAAf,GACHC,MAAM,CAACC,IAAP,CAAY,KAAKjB,SAAL,CAAe,CAAf,CAAZ,EAA+BkB,IAA/B,CAAqCC,GAAD,IAClC,CAAC,KAAKN,OAAO,CAACM,GAAD,CAAb,EAAoBC,WAApB,GAAkCC,QAAlC,CAA2C,KAAKC,MAAhD,CADF,CADG,GAIH,IAJJ;QAKF,CANA,MAMO,IAAI,KAAKA,MAAL,KAAgB,EAApB,EAAwB;UAC7B,OAAO,KAAKtB,SAAL,CAAee,MAAf,GACHC,MAAM,CAACC,IAAP,CAAY,KAAKjB,SAAL,CAAe,CAAf,CAAZ,EAA+BkB,IAA/B,CAAqCC,GAAD,IAClC,CAAC,KAAKN,OAAO,CAACM,GAAD,CAAb,EAAoBC,WAApB,GAAkCC,QAAlC,CAA2C,KAAKC,MAAhD,CADF,CADG,GAIH,IAJJ;QAKF,CANO,MAMA;UACL,OAAOT,OAAO,CAACC,MAAR,CAAeS,KAAf,CAAqB,KAAKT,MAA1B,CAAP;QACF;MACD,CAhBM,CAAP;IAiBD;EAnBO,CApBG;EA0CbU,OAAO,EAAE;IACPC,iBAAiB,GAAG;MAClBC,UAAU,CAAC,MAAM;QACf,KAAKC,UAAL,GAAkB,CAAC,KAAKA,UAAxB;QACA,KAAKC,aAAL,GAAqB,IAArB;MACD,CAHS,CAAV;IAID,CANM;;IAOPC,aAAa,CAACC,KAAD,EAAQ;MACnB,OAAOA,KAAK,CAACC,cAAN,EAAP;IACD;;EATM,CA1CI;;EAsDb9B,IAAI,GAAG;IACL,OAAO;MACLD,SAAS,EAAE,EADN;MAELsB,MAAM,EAAE,EAFH;MAGLU,QAAQ,EAAE,KAHL;MAILL,UAAU,EAAE,KAJP;MAKLC,aAAa,EAAE,KALV;MAMLd,MAAM,EAAE,EANH;MAOLlB,OAAO,EAAE,KAPJ;MAQLY,KAAK,EAAE,IARF;MASLJ,OAAO,EAAE;IATJ,CAAP;EAWD;;AAlEY,CAAf","names":["axios","name","mounted","pending","get","then","response","countries","data","forEach","con","capital","console","log","catch","error","finally","computed","filterAllCountries","filter","country","region","length","Object","keys","some","key","toLowerCase","includes","search","match","methods","handleFilterClick","setTimeout","showFilter","showAllRegion","formatNumbers","value","toLocaleString","ulFilter"],"sourceRoot":"","sources":["D:\\Challenges\\REST Countries API\\reast-countries-api\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"formDivSearch\">\n      <ion-icon id=\"searchIcon\" name=\"search-sharp\"></ion-icon>\n      <input\n        id=\"searchBox\"\n        v-model=\"search\"\n        type=\"text\"\n        placeholder=\"search for a country\"\n      />\n      <div class=\"forFilter\">\n        <a class=\"aFilter\" @click=\"ulFilter = !ulFilter\"\n          >Filter By Region <ion-icon name=\"chevron-down-sharp\"></ion-icon\n        ></a>\n        <ul v-if=\"ulFilter\">\n          <li>\n            <label for=\"radioAfrica\"\n              >Africa\n              <input\n                id=\"radioAfrica\"\n                class=\"dropdownInput\"\n                type=\"radio\"\n                name=\"africa\"\n                value=\"Africa\"\n                v-model=\"region\"\n                v-on:click=\"handleFilterClick\"\n              />\n            </label>\n          </li>\n          <li>\n            <input\n              id=\"radioAmerica\"\n              class=\"dropdownInput\"\n              type=\"radio\"\n              name=\"america\"\n              value=\"America\"\n              v-model=\"region\"\n              v-on:click=\"handleFilterClick\"\n            />\n            <label for=\"radioAmerica\">America</label>\n          </li>\n          <li>\n            <input\n              id=\"radioAsia\"\n              class=\"dropdownInput\"\n              type=\"radio\"\n              name=\"asia\"\n              value=\"Asia\"\n              v-model=\"region\"\n              v-on:click=\"handleFilterClick\"\n            />\n            <label for=\"radioAsia\">Asia</label>\n          </li>\n          <li>\n            <input\n              id=\"radioEurope\"\n              class=\"dropdownInput\"\n              type=\"radio\"\n              name=\"europe\"\n              value=\"Europe\"\n              v-model=\"region\"\n              v-on:click=\"handleFilterClick\"\n            />\n            <label for=\"radioEurope\">Europe</label>\n          </li>\n          <li>\n            <input\n              id=\"radioOceania\"\n              class=\"dropdownInput\"\n              type=\"radio\"\n              name=\"oceania\"\n              value=\"Oceania\"\n              v-model=\"region\"\n              v-on:click=\"handleFilterClick\"\n            />\n            <label for=\"radioOceania\">Oceania</label>\n          </li>\n          <!-- After selecting a region this \"All Regions\" option becomes available -->\n          <li v-if=\"showAllRegion\">\n            <input\n              id=\"radioAll\"\n              class=\"dropdownInput\"\n              type=\"radio\"\n              name=\"all\"\n              value=\"All Regions\"\n              v-model=\"region\"\n              v-on:click=\"handleFilterClick\"\n            />\n            <label for=\"radioAll\">All Regions</label>\n          </li>\n        </ul>\n      </div>\n    </div>\n\n    <h1 v-if=\"error !== null\">Sorry, an error has occurred {{ error }}</h1>\n    <!-- Loading API response -->\n    <div class=\"loaderFlex\"><div v-if=\"pending\" class=\"loader\"></div></div>\n\n    <div\n      v-if=\"countries\"\n      class=\"contentCardsCountry\"\n      style=\"display: flex; flex-wrap: wrap; justify-content: center\"\n    >\n      <div class=\"contentOfHome\">\n        <div class=\"featCard\">\n          <router-link :to=\"'/country/' + common\">\n            <div class=\"imgCard\">\n              <img :src=\"png\" alt=\"country\" />\n            </div>\n            <div class=\"detailCard\">\n              <span>{{ common }}</span>\n              <p>population: {{ formatNumbers(population) }}</p>\n              <p>region: {{ region }}</p>\n              <p>capital: {{ capital }}</p>\n            </div>\n          </router-link>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n\n\n\n\n\n\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"HomeView\",\n\n  async mounted() {\n    this.pending = true;\n    axios\n      .get(\"https://restcountries.com/v3.1/all\")\n      .then((response) => {\n        this.countries = response.data;\n        this.countries.forEach((con) => {\n          this.capital = con.capital[0];\n          console.log(this.capital);\n        });\n      })\n      .catch((error) => (this.error = error))\n      .finally(() => {\n        this.pending = false;\n      });\n  },\n\n  computed: {\n    filterAllCountries: function () {\n      return this.countries.filter((country) => {\n        if (this.region === \"\" || this.region === \"All Regions\") {\n          return this.countries.length\n            ? Object.keys(this.countries[0]).some((key) =>\n                (\"\" + country[key]).toLowerCase().includes(this.search)\n              )\n            : true;\n        } else if (this.search !== \"\") {\n          return this.countries.length\n            ? Object.keys(this.countries[0]).some((key) =>\n                (\"\" + country[key]).toLowerCase().includes(this.search)\n              )\n            : true;\n        } else {\n          return country.region.match(this.region);\n        }\n      });\n    },\n  },\n\n  methods: {\n    handleFilterClick() {\n      setTimeout(() => {\n        this.showFilter = !this.showFilter;\n        this.showAllRegion = true;\n      });\n    },\n    formatNumbers(value) {\n      return value.toLocaleString();\n    },\n  },\n\n  data() {\n    return {\n      countries: [],\n      search: \"\",\n      ulFilter: false,\n      showFilter: false,\n      showAllRegion: false,\n      region: \"\",\n      pending: false,\n      error: null,\n      capital: null,\n    };\n  },\n};\n</script>\n\n\n\n\n\n\n\n\n\n\n\n<style lang=\"scss\">\n.home {\n  height: 100%;\n  width: 100%;\n}\n\n.formDivSearch {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  padding: 30px 50px;\n  position: relative;\n  #searchIcon {\n    position: absolute;\n    color: var(--Blue-text);\n    font-size: 1.2rem;\n    left: 60px;\n    top: 40px;\n  }\n  #searchBox {\n    padding: 12px 12px 12px 40px;\n    border: none;\n    border-radius: 3px;\n    font-size: 0.8rem;\n    color: var(--Blue-text);\n    background-color: var(--elements);\n    outline: none;\n    appearance: none;\n    width: 420px;\n  }\n  #searchBox:focus {\n    color: var(--Blue-text);\n  }\n  #searchBox::placeholder {\n    color: var(--Blue-text);\n  }\n\n  .forFilter {\n    position: relative;\n    .aFilter {\n      padding: 15px;\n      background-color: var(--elements);\n      font-size: 0.8rem;\n      border-radius: 3px;\n      color: var(--Blue-text);\n      cursor: pointer;\n      ion-icon {\n        padding-left: 15px;\n      }\n    }\n    ul {\n      display: flex;\n      position: absolute;\n      flex-direction: column;\n      justify-content: flex-start;\n      align-items: center;\n      left: 0px;\n      top: 40px;\n      background-color: var(--elements);\n      border-radius: 7px;\n      z-index: 10;\n      li {\n        list-style: none;\n        padding: 10px 55px;\n        label {\n          color: var(--Blue-text);\n          font-size: 0.8rem;\n          cursor: pointer;\n        }\n        .dropdownInput {\n          display: none;\n        }\n      }\n    }\n  }\n\n  .contentCardsCountry {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n  }\n}\n\n@media (max-width: 700px) {\n  .formDivSearch #searchBox {\n    width: 100%;\n  }\n  #app header {\n    font-size: 0.8rem;\n  }\n  .home .formDivSearch {\n    flex-direction: column;\n    gap: 40px;\n  }\n}\n\n.contentOfHome {\n  padding: 40px 30px;\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: row;\n  gap: 75px;\n  justify-content: space-around;\n  .featCard {\n    background-color: var(--elements);\n    width: 260px;\n    height: auto;\n    border-radius: 5px;\n    a {\n      text-decoration: none;\n      .imgCard {\n        width: 100%;\n        height: 9em;\n        img {\n          width: 100%;\n          height: 100%;\n          border-top-left-radius: 5px;\n          border-top-right-radius: 5px;\n        }\n      }\n      .detailCard {\n        padding: 14px 15px 44px 15px;\n        display: flex;\n        flex-direction: column;\n        gap: 8px;\n        span {\n          font-weight: 600;\n          color: var(--Blue-text);\n          margin-bottom: 5px;\n          font-size: 1.3rem;\n        }\n        p {\n          font-weight: 300;\n          font-size: 1rem;\n          color: var(--Blue-text);\n        }\n      }\n    }\n  }\n}\n</style>\n\n"]},"metadata":{},"sourceType":"module"}