ry.population\"\n        :region=\"country.region\"\n        :png=\"country.flags.png\"\n      ></country-card>\n    </div>\n  </div>\n</template>\n\n\n\n\n\n\n\n\n\n<script>\nimport countryCard from \"../components/countries.vue\";\nimport { ref } from \"vue\";\n// import env from \"@/env.js\";\n\nexport default {\n  name: \"HomeView\",\n\n  components: {\n    // eslint-disable-next-line vue/no-unused-components\n    countryCard,\n  },\n\n  async mounted() {\n    fetch(\"https://restcountries.com/v3.1/all\")\n      .then((res) => res.json())\n      .then((response) => {\n        this.countries = response;\n      })\n      .catch((error) => {\n        console.log(\"Looks like there was a problem: \\n\", error);\n      });\n  },\n\n  async setup() {\n    const search = ref(\"\");\n    const changCon = ref([]);\n\n    const searchCountries = () => {\n      if (search.value != \"\") {\n        fetch(`https://restcountries.com/v3.1/name/${search.value}`)\n          .then((res) => res.json())\n          .then((data) => {\n            changCon.value = data.search;\n            console.log(changCon.value);\n          });\n      }\n    };\n    return {\n      searchCountries,\n    };\n  },\n\n  methods: {\n    ifSearch() {\n      if (this.search == \"\") {\n        this.countries = this.changCon.value;\n      } else {\n        return this.countries;\n      }\n    },\n  },\n\n  data() {\n    return {\n      countries: [],\n      search: \"\",\n    };\n  },\n  // computed: {\n  //   filtCountries: function () {\n  //     return this.countries.filter((country) =>\n  //       country.name.common.include(this.search)\n  //     );\n  //   },\n  // },\n\n  // computed: {\n  //   filtCountries: function () {\n  //     return this.countries.filter(country =>\n  //       country.incl\n  //     );\n  //   },\n  // },\n};\n</script>\n\n\n\n\n\n\n\n\n\n\n\n<style lang=\"scss\">\n.home {\n  height: 100%;\n  width: 100%;\n}\n\n.formDivSearch {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  padding: 30px 50px;\n  input {\n    padding: 12px;\n    border: none;\n    border-radius: 7px;\n    font-size: 0.8rem;\n    color: var(--Blue-text);\n    background-color: var(--elements);\n    outline: none;\n    appearance: none;\n  }\n\n  .forFilter {\n    position: relative;\n    .aFilter {\n      padding: 15px;\n      background-color: var(--elements);\n      font-size: 0.8rem;\n      border-radius: 7px;\n      color: var(--Blue-text);\n      cursor: pointer;\n      ion-icon {\n        padding-left: 15px;\n      }\n    }\n    ul {\n      display: none;\n      position: absolute;\n      flex-direction: column;\n      justify-content: flex-start;\n      align-items: center;\n      left: 0px;\n      top: 40px;\n      background-color: var(--elements);\n      border-radius: 7px;\n      z-index: 10;\n      li {\n        list-style: none;\n        padding: 10px 55px;\n        cursor: pointer;\n        a {\n          text-decoration: none;\n          color: var(--Blue-text);\n          font-size: 0.8rem;\n        }\n      }\n    }\n    ul.active {\n      display: flex;\n    }\n  }\n\n  .home .contentCardsCountry {\n    display: flex;\n    flex-wrap: wrap;\n  }\n}\n\n@media (max-width: 500px) {\n  #app header {\n    font-size: 0.8rem;\n  }\n  .home .formDivSearch {\n    flex-direction: column;\n    gap: 40px;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}