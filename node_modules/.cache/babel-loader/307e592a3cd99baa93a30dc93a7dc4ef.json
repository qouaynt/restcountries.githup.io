{"ast":null,"code":"import { ref } from \"vue\";\nexport default {\n  name: \"HomeView\",\n\n  setup() {\n    const search = ref(\"\");\n    const countries = ref([]);\n\n    const searchCountries = () => {\n      if (search.value != \"\") {\n        fetch(`https://restcountries.com/v3.1/name/${search.value}`).then(res => res.json()).then(data => {\n          console.log(data);\n          countries.value = data;\n        });\n      } else {\n        fetch(`https://restcountries.com/v3.1/all`).then(res => res.json()).then(data => {\n          console.log(data);\n          countries.value = data;\n        });\n      }\n    };\n\n    return {\n      search,\n      countries,\n      searchCountries\n    };\n  },\n\n  methods: {\n    activeUlFilter: function () {\n      let ul = document.querySelector(\".forFilter ul\");\n      ul.classList.toggle(\"active\");\n    }\n  }\n};","map":{"version":3,"mappings":"AA6CA,SAASA,GAAT,QAAoB,KAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;;EAEbC,KAAK,GAAG;IACN,MAAMC,MAAK,GAAIH,GAAG,CAAC,EAAD,CAAlB;IACA,MAAMI,SAAQ,GAAIJ,GAAG,CAAC,EAAD,CAArB;;IAEA,MAAMK,eAAc,GAAI,MAAM;MAC5B,IAAIF,MAAM,CAACG,KAAP,IAAgB,EAApB,EAAwB;QACtBC,KAAK,CAAE,uCAAsCJ,MAAM,CAACG,KAAM,EAArD,CAAL,CACGE,IADH,CACSC,GAAD,IAASA,GAAG,CAACC,IAAJ,EADjB,EAEGF,IAFH,CAESG,IAAD,IAAU;UACdC,OAAO,CAACC,GAAR,CAAYF,IAAZ;UACAP,SAAS,CAACE,KAAV,GAAkBK,IAAlB;QACD,CALH;MAMF,CAPA,MAOO;QACLJ,KAAK,CAAE,oCAAF,CAAL,CACGC,IADH,CACSC,GAAD,IAASA,GAAG,CAACC,IAAJ,EADjB,EAEGF,IAFH,CAESG,IAAD,IAAU;UACdC,OAAO,CAACC,GAAR,CAAYF,IAAZ;UACAP,SAAS,CAACE,KAAV,GAAkBK,IAAlB;QACD,CALH;MAMF;IACD,CAhBD;;IAiBA,OAAO;MACLR,MADK;MAELC,SAFK;MAGLC;IAHK,CAAP;EAKD,CA5BY;;EA6BbS,OAAO,EAAE;IACPC,cAAc,EAAE,YAAY;MAC1B,IAAIC,EAAC,GAAIC,QAAQ,CAACC,aAAT,CAAuB,eAAvB,CAAT;MAEAF,EAAE,CAACG,SAAH,CAAaC,MAAb,CAAoB,QAApB;IACD;EALM;AA7BI,CAAf","names":["ref","name","setup","search","countries","searchCountries","value","fetch","then","res","json","data","console","log","methods","activeUlFilter","ul","document","querySelector","classList","toggle"],"sourceRoot":"","sources":["D:\\Challenges\\REST Countries API\\rest-countries-api-with-color-theme-switcher-master\\reast-countries-api\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"formDivSearch\">\n      <input\n        @keypress=\"searchCountries\"\n        v-model=\"search\"\n        type=\"text\"\n        placeholder=\"search for a country\"\n      />\n      <div class=\"forFilter\">\n        <a class=\"aFilter\" @click=\"activeUlFilter\"\n          >Filter By Region <ion-icon name=\"chevron-down-sharp\"></ion-icon\n        ></a>\n        <ul>\n          <li><a>Africa</a></li>\n          <li><a>America</a></li>\n          <li><a>Asia</a></li>\n          <li><a>Europe</a></li>\n          <li><a>Oceania</a></li>\n        </ul>\n      </div>\n    </div>\n    <div class=\"contentOfHome\">\n      <div\n        class=\"featCard\"\n        v-for=\"country in countries\"\n        :key=\"country.numericCode\"\n      >\n        <router-link :to=\"'/countries/' + country.name.common\">\n          <div class=\"imgCard\">\n            <img :src=\"country.flags.png\" alt=\"country\" />\n          </div>\n          <div class=\"detailCard\">\n            <span>{{ country.name.common }}</span>\n            <p>population: {{ country.population }}</p>\n            <p>region: {{ country.region }}</p>\n            <p>capital: {{ country.capital[0] }}</p>\n          </div>\n        </router-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\n\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const search = ref(\"\");\n    const countries = ref([]);\n\n    const searchCountries = () => {\n      if (search.value != \"\") {\n        fetch(`https://restcountries.com/v3.1/name/${search.value}`)\n          .then((res) => res.json())\n          .then((data) => {\n            console.log(data);\n            countries.value = data;\n          });\n      } else {\n        fetch(`https://restcountries.com/v3.1/all`)\n          .then((res) => res.json())\n          .then((data) => {\n            console.log(data);\n            countries.value = data;\n          });\n      }\n    };\n    return {\n      search,\n      countries,\n      searchCountries,\n    };\n  },\n  methods: {\n    activeUlFilter: function () {\n      let ul = document.querySelector(\".forFilter ul\");\n\n      ul.classList.toggle(\"active\");\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.contentOfHome {\n  padding: 40px 45px;\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: row;\n  gap: 75px;\n  justify-content: space-around;\n  .featCard {\n    background-color: var(--elements);\n    width: 260px;\n    height: auto;\n    border-radius: 5px;\n    a {\n      text-decoration: none;\n      .imgCard {\n        width: 100%;\n        height: 9em;\n        img {\n          width: 100%;\n          height: 100%;\n          border-top-left-radius: 5px;\n          border-top-right-radius: 5px;\n        }\n      }\n      .detailCard {\n        padding: 15px;\n        span {\n          font-weight: 600;\n          color: var(--Blue-text);\n          margin-bottom: 15px;\n        }\n        p {\n          font-weight: 300;\n          color: var(--Blue-text);\n        }\n      }\n    }\n  }\n}\n\n.formDivSearch {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  padding: 30px 50px;\n  input {\n    padding: 12px;\n    border: none;\n    border-radius: 7px;\n    font-size: 0.8rem;\n    color: var(--Blue-text);\n    background-color: var(--elements);\n  }\n  input::selection {\n    border: red;\n  }\n\n  .forFilter {\n    position: relative;\n    .aFilter {\n      padding: 15px;\n      background-color: var(--elements);\n      font-size: 0.8rem;\n      border-radius: 7px;\n      color: var(--Blue-text);\n      cursor: pointer;\n      ion-icon {\n        padding-left: 15px;\n      }\n    }\n    ul {\n      display: none;\n      position: absolute;\n      flex-direction: column;\n      justify-content: flex-start;\n      align-items: center;\n      left: 0px;\n      top: 40px;\n      background-color: var(--elements);\n      border-radius: 7px;\n      z-index: 10;\n      li {\n        list-style: none;\n        padding: 10px 59px;\n        cursor: pointer;\n        a {\n          text-decoration: none;\n          color: var(--Blue-text);\n          font-size: 0.8rem;\n        }\n      }\n    }\n    ul.active {\n      display: flex;\n    }\n  }\n}\n\n@media (max-width: 500px) {\n  #app header {\n    font-size: 0.8rem;\n  }\n  .home .formDivSearch {\n    flex-direction: column;\n    gap: 40px;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}